{
  "nodeModulesDir": "auto",
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "start": "deno run -A _fresh/compiled-entry.js",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "unstable": [
    "kv",
    "fmt-component",
    "fmt-sql",
    "cron"
  ],
  "lint": {
    "rules": {
      "exclude": [
        "require-yield"
      ],
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@elysiajs/swagger": "npm:@elysiajs/swagger@^1.3.1",
    "@kitsonk/kv-toolbox": "jsr:@kitsonk/kv-toolbox@^0.30.0",
    "@kubb/cli": "npm:@kubb/cli@^3.18.1",
    "@kubb/core": "npm:@kubb/core@^3.18.1",
    "@kubb/plugin-oas": "npm:@kubb/plugin-oas@^3.18.1",
    "@kubb/plugin-ts": "npm:@kubb/plugin-ts@^3.18.1",
    "@std/datetime": "jsr:@std/datetime@^0.225.5",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/http": "jsr:@std/http@^1.0.20",
    "@std/yaml": "jsr:@std/yaml@^1.0.9",
    "@xstate/react": "https://esm.sh/@xstate/react?alias=react:preact/compat&deps=react&external=preact",
    "clsx": "npm:clsx@^2.1.1",
    "effect": "npm:effect@^3.17.7",
    "elysia": "npm:elysia@^1.3.13",
    "fresh": "jsr:@fresh/core@^2.0.0-alpha.58",
    "google-auth-library": "npm:google-auth-library@^9.15.1",
    "preact": "npm:preact@^10.27.1",
    "@preact/signals": "npm:@preact/signals@^2.2.1",
    "tailwind-merge": "npm:tailwind-merge@^3.3.1",
    "tailwindcss": "npm:tailwindcss@^4.1.10",
    "@fresh/plugin-tailwind": "jsr:@fresh/plugin-tailwind@^0.0.1-alpha.9",
    "@tailwindcss/postcss": "npm:@tailwindcss/postcss@^4.1.10",
    "postcss": "npm:postcss@^8.5.6",
    "xstate": "npm:xstate@^5.20.2"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns",
      "deno.unstable"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head"
    ]
  },
  "version": "0.1.1"
}
